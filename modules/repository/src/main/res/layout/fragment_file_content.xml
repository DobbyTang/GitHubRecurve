<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <import type="androidx.lifecycle.LiveData"/>
        <import type="com.tangpj.repository.entry.vo.FileContent"/>
        <import type="com.tangpj.recurve.resource.Resource"/>

        <variable
            name="fileContent"
            type="LiveData&lt;Resource&lt;FileContent>>" />
    </data>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_file_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:md="@{fileContent.data.content}"/>

</layout>
